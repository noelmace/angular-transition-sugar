{
  "name": "angularjs-decorators",
  "version": "0.2.2",
  "description": "Angular 2 inspired decorators for angularjs",
  "main": "dist/Component.js",
  "scripts": {
    "build": "jspm bundle-sfx src/core/index.js dist/core.js && jspm bundle-sfx src/platform-browser-dynamic/index.js dist/platform-browser-dynamic.js",
    "babelify": "babel src --out-dir dist/all",
    "prepublish": "npm run build",
    "version": "npm run build && git add --force dist/ && git add package.json",
    "postversion": "git rm -r dist/ && git commit -m 'CLEAN AFTER RELEASE'",
    "link": "npm run build && jspm link github:noelmace/angularjs-decorators@dev -y"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noelmace/angularjs-decorators.git"
  },
  "author": "Noël Macé <contact@noelmace.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/noelmace/angularjs-decorators/issues"
  },
  "homepage": "https://github.com/noelmace/angularjs-decorators#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-plugin-syntax-decorators": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.5.0",
    "jspm": "^0.16.39"
  },
  "jspm": {
    "registry": "jspm",
    "directories": {
      "lib": "dist"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    },
    "peerDependencies": {
      "angular": "github:angular/bower-angular@>=1.5.0 < 2.0.0"
    }
  }
}
